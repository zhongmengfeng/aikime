<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- 填充颜色 -->
    <solid android:color="@color/white"/>

    <!-- 设置圆角半径 -->
    <corners
        android:radius="5dp"/>

    <!-- 描边 -->
    <stroke
        android:width="1dp"
        android:color="@android:color/black"
        android:dashWidth="1dp"
        android:dashGap="0dp"/>

</shape>